    <DataSources isList="true" count="2">
      <dsBill Ref="3" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>dsBill</Alias>
        <Columns isList="true" count="8">
          <value>billtype,System.Int32</value>
          <value>billno,System.String</value>
          <value>billofladingno,System.String</value>
          <value>billdate,System.DateTime</value>
          <value>consigneename,System.String</value>
          <value>billingaddress,System.String</value>
          <value>vesselname,System.String</value>
          <value>voyageno,System.String</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>dsBill</Name>
        <NameInSource>cn</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT
	bill.billtype, 
	bill.billno,
	bill.billofladingno,
	bill.billdate,
	consigneedetail.name as consigneename,
	bill.billingaddress,
	vesseldetail.name as vesselname,
	bill.voyageno
	FROM bill
	inner join contact consigneedetail
		on consigneedetail.contact = bill.consignee	
	inner join contact vesseldetail
		on vesseldetail.contact = bill.vessel		
WHERE bill.bill = '{pId}'</SqlCommand>
      </dsBill>
      <dsBillItem Ref="4" type="Stimulsoft.Report.Dictionary.StiSqlSource" isKey="true">
        <Alias>dsBillItem</Alias>
        <Columns isList="true" count="8">
          <value>bill,System.Int32</value>
          <value>quantity,System.Int32</value>
          <value>unitname,System.String</value>
          <value>cargoname,System.String</value>
          <value>rate,System.Decimal</value>
          <value>amount,System.Decimal</value>
          <value>vatamount,System.Decimal</value>
          <value>totalbill,System.Decimal</value>
        </Columns>
        <CommandTimeout>30</CommandTimeout>
        <Dictionary isRef="1" />
        <Name>dsBillItem</Name>
        <NameInSource>cn</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>WITH billamount AS (
	SELECT bill, SUM(ROUND(rate * quantity * 0.12, 2)) as vatamount
	FROM billitemroro
	WHERE billitemroro.bill = '{pId}'
	GROUP BY bill
	)
SELECT
	roro.bill,
	roro.quantity,
	uom.name as unitname,
	item.itemname as cargoname,
	roro.rate,
	Round(roro.rate * roro.quantity, 2) AS amount,
	billamount.vatamount,
	bill.totalbill
	FROM bill
	inner join billitemroro roro
		on roro.bill = bill.bill
	inner join item
		on item.item = roro.cargo
	inner join billamount
		on billamount.bill = bill.bill
	inner join uom
		on uom.uom = roro.uom	
WHERE roro.bill = '{pId}'		</SqlCommand>
      </dsBillItem>
